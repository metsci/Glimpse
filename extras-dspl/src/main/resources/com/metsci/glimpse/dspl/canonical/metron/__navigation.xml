<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<dspl targetNamespace="http://www.metsci.com/dspl/navigation" xmlns="http://schemas.google.com/dspl/2010"
	xmlns:geo="http://www.google.com/publicdata/dataset/google/geo"
	xmlns:time="http://www.google.com/publicdata/dataset/google/time"
	xmlns:quantity="http://www.google.com/publicdata/dataset/google/quantity"
	xmlns:entity="http://www.google.com/publicdata/dataset/google/entity"
	xmlns:physical="http://www.metsci.com/dspl/__physical_unit">

	<import namespace="http://www.google.com/publicdata/dataset/google/time" />
	<import namespace="http://www.google.com/publicdata/dataset/google/quantity" />
	<import namespace="http://www.google.com/publicdata/dataset/google/entity" />
	<import namespace="http://www.google.com/publicdata/dataset/google/geo" />
	<import namespace="http://www.metsci.com/dspl/__physical_unit" />

	<info>
		<name>
			<value xml:lang="en">Navigation concepts</value>
		</name>
		<description>
			<value xml:lang="en">Concepts for describing navigation and track related data.
			</value>
		</description>
	</info>

	<provider>
		<name>
			<value xml:lang="en">Metron Inc.</value>
		</name>
		<description>
			<value xml:lang="en">Metron Inc.</value>
		</description>
		<url>
			<value xml:lang="en">http://www.metsci.com</value>
		</url>
	</provider>

	<concepts>
	    <concept id="latitude" extends="physical:physical_magnitude">
            <info>
                <name>
                    <value>Latitude</value>
                </name>
                <description>
                    <value>Latitude measured positively toward North</value>
                </description>
            </info>
        </concept>

        <concept id="latitude_deg" extends="latitude">
            <attribute concept="physical:physical_unit">
                <value>DEGREES</value>
            </attribute>
        </concept>

        <concept id="latitude_rad" extends="latitude">
            <attribute concept="physical:physical_unit">
                <value>RADIANS</value>
            </attribute>
        </concept>

        <concept id="longitude" extends="physical:physical_magnitude">
            <info>
                <name>
                    <value>Longitude</value>
                </name>
                <description>
                    <value>Longitude measured positively toward East</value>
                </description>
            </info>
        </concept>

        <concept id="longitude_deg" extends="longitude">
            <attribute concept="physical:physical_unit">
                <value>DEGREES</value>
            </attribute>
        </concept>

        <concept id="longitude_rad" extends="longitude">
            <attribute concept="physical:physical_unit">
                <value>RADIANS</value>
            </attribute>
        </concept>

        <concept id="true_bearing" extends="physical:physical_magnitude">
        </concept>

        <concept id="magnetic_bearing" extends="physical:physical_magnitude">
        </concept>

        <concept id="relative_bearing" extends="physical:physical_magnitude">
        </concept>

	</concepts>

	<tables>
	</tables>
</dspl>

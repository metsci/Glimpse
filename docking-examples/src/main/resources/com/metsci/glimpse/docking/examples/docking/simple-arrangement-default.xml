<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<group xmlns="http://glimpse.metsci.com/docking" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <frames>
        <frame>
            <x>100</x>
            <y>100</y>
            <width>1024</width>
            <height>768</height>
            <isMaximizedHoriz>false</isMaximizedHoriz>
            <isMaximizedVert>false</isMaximizedVert>
            <docker xsi:type="Split">
                <arrangeVertically>true</arrangeVertically>
                <splitFrac>0.75</splitFrac>
                <childA xsi:type="Split">
                    <arrangeVertically>false</arrangeVertically>
                    <splitFrac>0.25</splitFrac>
                    <childA xsi:type="Tile">
                        <views>
                            <view>aView</view>
                            <view>bView</view>
                        </views>
                        <selectedView>aView</selectedView>
                        <isMaximized>false</isMaximized>
                    </childA>
                    <childB xsi:type="Tile">
                        <views>
                            <view>cView</view>
                            <view>dView</view>
                        </views>
                        <selectedView>cView</selectedView>
                        <isMaximized>false</isMaximized>
                    </childB>
                </childA>
                <childB xsi:type="Tile">
                    <views>
                        <view>eView</view>
                        <view>fView</view>
                        <view>gView</view>
                        <view>hView</view>
                    </views>
                    <selectedView>eView</selectedView>
                    <isMaximized>false</isMaximized>
                </childB>
            </docker>
        </frame>
    </frames>
</group>
